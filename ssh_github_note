#1 
ssh-keygen -t rsa -b 4096 -C sansemail@pasdemail.com

#2
eval "$(ssh-agent -s)"

#3
ssh-add ~/.ssh/id_rsa

#4
Copy your public SSH key  

cat ~/.ssh/id_rsa.pub

#5 edit ~/.ssh/config
Host github.com-activehacker
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa

#6 test connection 

ssh -T git@github.com

add permanent host > yes


#7 copy email anonyme depuis settings de github, le set avec 
  git config --global user.email "xxxxxx.noreply.github.com"
  git config --global user.name "xxxx"


#8 git init

git init
git remote add origin git@github.com:xxxx/yyyyy.git

git remote -v
git add --all
git commit -am "Initial commit"
git push -f origin master


